<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<!--f=f(x,t,u)-->
		<add key="K" value="x*x*t+u*u"/>

		<!--f=f(x,t,u)-->
		<add key="dK_du" value="2*u"/>

		<!--f=f(x,t,u,K)-->
		<add key="g" value="x*x+6*x*t-2*u*(2*x*t+3*t*t)*(2*x*t+3*t*t)-2*t*K"/>

		<!--f=f(x)-->
		<add key="InitCond" value="0"/>

		<!--f=f(t)-->
		<add key="LeftBoundCond" value="0"/>

		<!--f=f(t)-->
		<add key="RightBoundCond" value="t+3*t*t"/>

		<!--f=f(x,t)-->
		<add key="u" value="x*x*t+3*x*t*t"/>

		<!--f=f(x,t)-->
		<add key="du_dx" value="2*x*t+3*t*t"/>

		<!--f=f(x,t)-->
		<add key="d2u_dx2" value="2*t"/>

		<!--f=f(x,t)-->
		<add key="du_dt" value="x*x+6*x*t"/>

		<add key="X1" value="0"/>
	</appSettings>
</configuration>